{
    "version": 2,
    "builds": [
        {
            "src": "saincube_portal/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "installCommand": "pip3 install -r requirements.txt"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        }
    ],
        "routes": [
          {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
          },
          {
            "src": "/(.*)",
            "dest": "saincube_portal/wsgi.py"
          }
        ]
  }
  